{
  "name": "@react-querybuilder/antd",
  "description": "Custom Ant Design components for react-querybuilder",
  "version": "7.0.1",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/react-querybuilder_antd.legacy-esm.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/react-querybuilder_antd.d.mts",
        "default": "./dist/react-querybuilder_antd.mjs"
      },
      "require": {
        "types": "./dist/cjs/react-querybuilder_antd.cjs.development.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/cjs/react-querybuilder_antd.cjs.production.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/antd"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "bunx --bun vite",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.3.1",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.63",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "antd": "^5.15.1",
    "dayjs": "^1.11.10",
    "rc-picker": "^4.2.0",
    "rc-select": "^14.12.0",
    "rc-util": "^5.39.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-querybuilder": "^7.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.5"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=4.7.0",
    "antd": ">=4.10.0",
    "dayjs": ">=1",
    "react": ">=16.8.0",
    "react-querybuilder": "^7.0.1"
  }
}
